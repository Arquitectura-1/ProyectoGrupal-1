module OutMem (input logic [31:0] Result);

	int pixCounter=0;
	reg [7:0] OutMemory1 [0:10000];

		always@(Result)
			if (pixCounter < 10000) begin
				OutMemory1[pixCounter]=Result[31:0];
				pixCounter = pixCounter +1;
			end
			else begin
				$writememb("C:\\Users\\bryan\\Desktop\\SegundoProyecto\\OutMemory1.txt",OutMemory1);
			end
			
endmodule 