module MEM (input logic [6:0] RdOut, branchResult, input logic [31:0] AluResult, input logic [5;0] OpCode,output logic [31:0] Result,
output logic [6:0] RdWb,output logic Wrenable,output logic [6:0] BranchResultOut);

reg [8:0] DataMemory [0:615000];
$readmemh("Pixel.txt", DataMemory);
int Addr;

logic [31:0] tmpResult;

always @(OpCode) begin

	if(OpCode == 6) begin
	
		Addr = AluResult;
		Result = DataMemory[Addr];
		RdWb = RdOut;
		BranchResultOut = branchResult;
		Wrenable =1;
		
	end
end

endmodule 